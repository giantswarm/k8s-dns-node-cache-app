{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
      "name": {
          "type": "string"
      },
      "namespace": {
          "type": "string"
      },
      "serviceType": {
          "type": "string"
      },
      "image": {
          "type": "object",
          "properties": {
              "registry": {
                  "type": "string"
              },
              "name": {
                  "type": "string"
              },
              "tag": {
                  "type": "string"
              }
          }
      },
      "updateStrategy": {
          "type": "object",
          "properties": {
              "rollingUpdate": {
                  "type": "object",
                  "properties": {
                      "maxUnavailable": {
                          "type": "string"
                      }
                  }
              }
          }
      },
      "cluster": {
          "type": "object",
          "properties": {
              "kubernetes": {
                  "type": "object",
                  "properties": {
                      "DNS": {
                          "type": "object",
                          "properties": {
                              "IP": {
                                  "type": "string"
                              }
                          }
                      },
                      "LOCAL": {
                          "type": "object",
                          "properties": {
                              "IP": {
                                  "type": "string"
                              }
                          }
                      },
                      "clusterDomain": {
                          "type": "string"
                      }
                  }
              }
          }
      },
      "ports": {
          "type": "object",
          "properties": {
              "prometheus": {
                  "type": "object",
                  "properties": {
                      "nodecache": {
                          "type": "integer"
                      },
                      "coredns": {
                          "type": "integer"
                      }
                  }
              },
              "liveness": {
                  "type": "integer"
              }
          }
      },
      "upstreamService": {
          "type": "object",
          "properties": {
              "servicePort": {
                  "type": "integer"
              },
              "podPort": {
                  "type": "integer"
              },
              "appName": {
                  "type": "string"
              }
          }
      },
      "resources": {
          "type": "object",
          "properties": {
              "limits": {
                  "type": "object",
                  "properties": {
                      "cpu": {
                          "type": "string"
                      },
                      "memory": {
                          "type": "string"
                      }
                  }
              },
              "requests": {
                  "type": "object",
                  "properties": {
                      "cpu": {
                          "type": "string"
                      },
                      "memory": {
                          "type": "string"
                      }
                  }
              }
          }
      },
      "configmap": {
          "type": "object",
          "properties": {
              "cache": {
                  "type": "object",
                  "properties": {
                      "success": {
                          "type": "integer"
                      },
                      "denial": {
                          "type": "integer"
                      }
                  }
              }
          }
      },
      "verticalPodAutoscaler": {
          "type": "object",
          "properties": {
              "enabled": {
                  "type": "boolean"
              }
          }
      },
      "test": {
          "type": "object",
          "properties": {
              "image": {
                  "type": "object",
                  "properties": {
                      "name": {
                          "type": "string"
                      },
                      "tag": {
                          "type": "string"
                      }
                  }
              }
          }
      }
  }
}